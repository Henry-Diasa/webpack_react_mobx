
{
  "parser":"babel-eslint",
  "env":{
    "browser": true,
    "es6":true,
    "node":true
  },
  "parserOptions":{
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "ecmaVersion":7,
    "sourceType": "module"
  },
  "extends":"airbnb",
  "rules":{
    "semi":[0],
    "react/jsx-filename-extension":[0],
    "global-require":[0],
    "jsx-a11y/anchor-is-valid": [0],
    "react/require-default-props": [0],
    "arrow-body-style": [0]
  }
}
